public class ExprimentFour {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		boolean a;
		boolean x;
		boolean y;
		boolean z;
		a = 5>6;
		x =! a;
		y = a&&x;
		z = x||y;
		System.out.println("a="+a);
		System.out.println("x="+x);
		System.out.println("y="+y);
		System.out.println("z="+z);
	}

}
