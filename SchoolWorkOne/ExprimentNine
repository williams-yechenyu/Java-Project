public class ExprimentNine {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int s = 0;
		for(int i = 1; i <= 99; i++)
		{
			s += i;
			i++;
		}
		System.out.println("1+3+5+...+99="+s);
//		
//		for(int j = 1; j<5; j++)
//		{
//			System.out.println("j = "+ j );
//		}
//		j=1;
	}

}
